<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="525.0" prefWidth="497.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gui.MainMenu">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="145.0" layoutY="116.0" prefHeight="67.0" prefWidth="211.0" text="Wybierz poziom" textAlignment="CENTER">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button id="startButton" fx:id="startButton" layoutX="156.0" layoutY="434.0" mnemonicParsing="false" onAction="#gameStart" prefHeight="56.0" prefWidth="188.0" text="Start" />
      <MenuButton id="levelBar" fx:id="levelBar" layoutX="173.0" layoutY="210.0" mnemonicParsing="false" prefHeight="46.0" prefWidth="155.0" text="Wybierz Level">
        <items>
          <MenuItem mnemonicParsing="false" text="Very Easy" onAction="#veryEasy"/>
            <MenuItem mnemonicParsing="false" text="Easy" onAction="#easy"/>
            <MenuItem mnemonicParsing="false" text="Medium" onAction="#medium"/>
            <MenuItem mnemonicParsing="false" text="Hard" onAction="#hard"/>
        </items>
      </MenuButton>
   </children>
</AnchorPane>
