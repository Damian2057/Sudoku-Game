<AnchorPane prefHeight="530.0" prefWidth="500.0" styleClass="bodybg" stylesheets="@../../../img/style.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gui.MainMenu">
